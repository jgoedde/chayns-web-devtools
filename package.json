{
  "name": "chayns-web-devtools",
  "version": "1.0.1",
  "description": "Chrome Extension for reading chayns environment from chayns sites.\n\n",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jgoedde/chayns-web-devtools.git"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:firefox": "tsc --noEmit && cross-env __FIREFOX__=true vite build",
    "build:watch": "cross-env __DEV__=true vite build -w --mode development",
    "build:firefox:watch": "cross-env __DEV__=true __FIREFOX__=true vite build -w --mode development",
    "build:hmr": "rollup --config utils/reload/rollup.config.ts",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "pnpm build:hmr && (run-p wss build:watch)",
    "dev:firefox": "pnpm build:hmr && (run-p wss build:firefox:watch)",
    "test": "jest",
    "commitlint": "commitlint --edit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "pnpm lint -- --fix",
    "prettier": "prettier . --write"
  },
  "type": "module",
  "dependencies": {
    "@mantine/core": "^7.1.7",
    "@mantine/hooks": "^7.1.7",
    "@mantine/notifications": "^7.1.7",
    "@tabler/icons-react": "^2.40.0",
    "@testing-library/jest-dom": "^6.1.4",
    "construct-style-sheets-polyfill": "^3.1.0",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "lodash-humps": "^3.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.2.4",
    "use-chrome-storage": "^1.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@rollup/plugin-typescript": "^11.1.5",
    "@testing-library/react": "13.4.0",
    "@twind/core": "^1.1.3",
    "@twind/preset-autoprefix": "^1.0.7",
    "@twind/preset-tailwind": "^1.1.4",
    "@types/chrome": "0.0.248",
    "@types/jest": "29.0.3",
    "@types/lodash": "^4.14.200",
    "@types/node": "20.10.3",
    "@types/react": "18.2.29",
    "@types/react-dom": "18.2.13",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "4.1.0",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "11.1.0",
    "husky": "^8.0.3",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rollup": "2.79.1",
    "sass": "1.62.1",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.0",
    "tslib": "^2.6.2",
    "typescript": "4.9.5",
    "vite": "4.5.0",
    "ws": "8.13.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "packageManager": "pnpm@8.9.2"
}
